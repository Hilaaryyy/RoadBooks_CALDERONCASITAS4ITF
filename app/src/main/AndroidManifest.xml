<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.roadbooks.project">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Review"></activity>
        <activity android:name=".Quiz"></activity>
        <activity android:name=".Grades"></activity>
        <activity android:name=".emergencies"></activity>
        <activity android:name=".Parking"></activity>
        <activity android:name=".Violations"></activity>
        <activity android:name=".Handling"></activity>
        <activity android:name=".Signs"></activity>
        <activity android:name=".GenKnow"></activity>
        <activity android:name=".Positions"></activity>
        <activity android:name=".QuizActivity"></activity>
        <activity android:name=".StartingScreenActivity"></activity>
        <activity android:name=".EmerQuizActivity"></activity>
        <activity android:name=".SignsQuizActivity"></activity>
        <activity android:name=".ViolationsQuizActivity"></activity>
        <activity android:name=".HandlingQuizActivity"></activity>
        <activity android:name=".RoadQuizActivity"></activity>
        <activity android:name=".ParkingQuizActivity"></activity>
        <activity android:name=".RandomQuizActivity"></activity>
        <service android:name=".CustomService"></service>
    </application>

</manifest>